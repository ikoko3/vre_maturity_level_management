  "components": {
    "schemas": {
      "labCreationSchema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Dolphin watching Lab"
          },
          "alias": {
            "type": "string",
            "example": "DLPH-01"
          },
          "parent_lab_id": {
            "type": "string",
            "example": ""
          }
        },
        "required": [
          "name",
          "alias",
          "parent_lab_id"
        ]
      },
      "conditionUpdateSchema": {
        "type": "object",
        "properties": {
          "is_fullfilled": {
            "type": "boolean",
            "example": true
          },
          "comments": {
            "type": "string",
            "example": ""
          }
        },
        "required": [
          "is_fullfilled",
          "comments"
        ]
      },
      "roleSchema": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "VLD"
          },
          "name": {
            "type": "string",
            "example": "Virtual Lab Developer"
          },
          "description": {
            "type": "string",
            "example": "Develops Virtual Labs"
          },
          "is_global": {
            "type": "boolean",
            "example": false
          }
        },
        "required": [
          "code",
          "name",
          "description",
          "is_global"
        ]
      },
      "userSchema": {
        "type": "object",
        "properties": {
          "reference_id": {
            "type": "string",
            "example": "12345"
          },
          "email": {
            "type": "string",
            "example": "test@test.nl"
          },
          "name": {
            "type": "string",
            "example": "Joe Doe"
          },
          "global_roles": {
            "type": "string",
            "example": "['VLD']"
          }
        },
        "required": [
          "reference_id",
          "email",
          "name",
          "global_roles"
        ]
      },
      "userRolesSchema": {
        "type": "object",
        "properties": {
          "roles": {
            "type": "string",
            "example": "['VLD']"
          }
        },
        "required": [
          "roles"
        ]
      },
      "labRoleSchema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "user_id": {
              "type": "string",
              "example": "68417f10dbbf0245911c648c"
            },
            "role_code": {
              "type": "string",
              "example": "VLD"
            }
          },
          "required": [
            "user_id",
            "role_code"
          ]
        }
      }
    }
  }
